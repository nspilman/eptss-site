{
  "$schema": "https://openapi.vercel.sh/vercel.json",
  "buildCommand": "cd ../.. && turbo build --filter=@eptss/web",
  "installCommand": "cd ../.. && bun install",
  "framework": "nextjs",
  "outputDirectory": ".next",
  "rewrites": [
    {
      "source": "/admin",
      "destination": "https://eptss-admin.vercel.app"
    },
    {
      "source": "/admin/:path*",
      "destination": "https://eptss-admin.vercel.app/:path*"
    }
  ]
}
